<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.wttech.vmp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>vmp-client</name>
  <description>
        An Ionic Framework and Cordova project.
    </description>
  <author email="hi@ionicframework" href="http://ionicframework.com/">
      Ionic Framework Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <access origin="tel:*" launch-external="yes"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="3000"/>
  <preference name="Orientation" value="portrait"/>
  <preference name="StatusBarBackgroundColor" value="#ffffff"/>
  <preference name="StatusBarOverlaysWebView" value="true"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <icon src="resources/logo.png"/>
  <platform name="android">
    <splash src="resources/launcher.jpg" density="land-ldpi"/>
    <splash src="resources/launcher.jpg" density="land-mdpi"/>
    <splash src="resources/launcher.jpg" density="land-hdpi"/>
    <splash src="resources/launcher.jpg" density="land-xhdpi"/>
    <splash src="resources/launcher.jpg" density="land-xxhdpi"/>
    <splash src="resources/launcher.jpg" density="land-xxxhdpi"/>
    <splash src="resources/launcher.jpg" density="port-ldpi"/>
    <splash src="resources/launcher.jpg" density="port-mdpi"/>
    <splash src="resources/launcher.jpg" density="port-hdpi"/>
    <splash ssrc="resources/launcher.jpg" density="port-xhdpi"/>
    <splash src="resources/launcher.jpg" density="port-xxhdpi"/>
    <splash src="resources/launcher.jpg" density="port-xxxhdpi"/>
  </platform>
  <platform name="ios">
    <splash height="480" src="resources/launcher.jpg" width="320"/>
    <splash height="960" src="resources/launcher.jpg" width="640"/>
    <splash height="1024" src="resources/launcher.jpg" width="768"/>
    <splash height="2048" src="resources/launcher.jpg" width="1536"/>
    <splash height="768" src="resources/launcher.jpg" width="1024"/>
    <splash height="1536" src="resources/launcher.jpg" width="2048"/>
    <splash height="1136" src="resources/launcher.jpg" width="640"/>
    <splash height="1334" src="resources/launcher.jpg" width="750"/>
    <splash height="2208" src="resources/launcher.jpg" width="1242"/>
    <splash height="1242" src="resources/launcher.jpg" width="2208"/>
  </platform>
</widget>
